{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13
  },
  "plugins": ["react"],
  "rules": {
    "quotes": [2, "double", { "avoidEscape": true }],
    "linebreak-style": 0,
    "implicit-arrow-linebreak": 0,
    "comma-dangle": ["error", "never"],
  }
}
